{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": [
            true,
            {
                "semi": false,
                "trailingComma": "es5",
                "jsxBracketSameLine": true
            }
        ]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}
